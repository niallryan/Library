<!-- http://teamtreehouse.com/library/building-social-features-in-ruby-on-rails-2/adding-state/test-factories-2 -->

<div class="well">

<% @user_friendships.each do |u_f| %>
    <div>
    <% friend = u_f.friend %>
    <%= link_to image_tag(friend.profile_pic.url), profile_path(friend) %>
    <%= friend.profile_name %>
    </div>
<% end %>

</div>