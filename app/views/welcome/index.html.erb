Hello

<% if user_signed_in? %>
    <h3>Search For Users</h3>
    <%= form_tag @user, :method => 'get' do %>
        <p>
          <%= text_field_tag :search_query, params[:search_query] %>
          <%= submit_tag "Search", :name => nil %>
        </p>
    <% end %>
    <% if @users %>
        <% @users.each do |user| %>
            <%= link_to "#{user.profile_name}", profile_path(user) %>
        <% end %>
    <% end %>
<% end %>